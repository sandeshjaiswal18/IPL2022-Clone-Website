    .ov-card {
        background: #FFFFFF;
        border-radius: 4px;
        border: 2px solid rgba(0 0 0 / 5%);
        box-shadow: 2px 2px 5px #dee2e6;
    }

    .ov-card b {
        color: #18184A;
    }

    .team-selection {
        box-shadow: 0px 4.00779px 8.01557px rgba(0, 0, 0, 0.25);
        border-radius: 10px;
    }

    .team-selection>div {
        width: calc(100% - 27px);
        text-transform: uppercase;
        float: left;
        padding-right: 10px;
    }

    .team-selection span img {
        width: 5%;
    }

    .ov-card .progress {
        height: 2rem;
        background: #FFFFFF;
        box-shadow: 0px 4.00779px 8.01557px rgba(0, 0, 0, 0.25);
    }

    .ov-card .progress-bar {
